{
  "name": "umlbot",
  "description": "UML genrator bot for Slack on top PlantUML",
  "repository": "https://github.com/taichi/umlbot",
  "keywords": ["java8", "siden", "uml"],
  "env": {
    "URL": {
      "description": "heroku application url. like http://example.herokuapp.com"
    },
    "TOKEN": {
      "description": "slack sent in the outgoing payload."
    },
    "SIDEN_ENV": {
      "description": "siden running mode",
      "value": "production"
    }
  }
}
